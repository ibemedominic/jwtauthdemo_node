{
  "name": "jwtcommunicator",
  "private": true,
  "version": "1.0.0",
  "description": "JWT Communicator",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc ",
    "debug": "./node_modules/.bin/nodemon -L --config nodemon-docker-debug.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:prod": "node dist/main",
    "standalone": "tsc && cd ./dist && node dist/main ",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@ladjs/graceful": "^1.1.5",
    "@types/amqplib": "^0.8.2",
    "@types/body-parser": "^1.19.2",
    "@types/body-parser-xml": "^2.0.2",
    "axios": "^1.3.2",
    "body-parser-xml": "^2.0.3",
    "bree": "^8.0.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "crypto": "^1.0.1",
    "jose": "^4.11.4",
    "winston": "^3.8.1",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.42",
    "@types/supertest": "^2.0.12",
    "@types/yargs": "^17.0.22",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "dotenv": "^16.0.1",
    "nodemon": "^2.0.18",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.20",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.7.4"
  }
}
